/*
DROP TABLE TB_CMIT_MNG CASCADE CONSTRAINTS PURGE;

DROP TABLE TB_MFCMM_TENURE CASCADE CONSTRAINTS PURGE;

DROP TABLE TB_MFCMM CASCADE CONSTRAINTS PURGE;

DROP TABLE TB_CMIT_DMGE CASCADE CONSTRAINTS PURGE;

CREATE TABLE TB_CMIT_DMGE
(
    CMIT_MNG_NO          NUMBER(10) NOT NULL ,
    BIZ_AREA_CD          VARCHAR2(10) NULL ,
    DMGE_SE_CD           VARCHAR2(2) NULL ,
    CMIT_SE_CD           VARCHAR2(2) NULL ,
    EXMN_SE_CD           VARCHAR2(2) NULL ,
    CMIT_ODER            VARCHAR2(20) NULL ,
    CMIT_NM              VARCHAR2(100) NULL ,
    OPMT_BGNG_YMD        VARCHAR2(8) NULL ,
    OPMT_END_YMD         VARCHAR2(8) NULL ,
    RGTR_NO              VARCHAR2(20) NULL ,
    REG_YMD              DATE NULL ,
    MDFR_NO              VARCHAR2(20) NULL ,
    MDFCN_YMD            DATE NULL 
);

ALTER TABLE TB_CMIT_DMGE
    ADD CONSTRAINT  PK_TB_CMIT_DMGE PRIMARY KEY (CMIT_MNG_NO);

CREATE TABLE TB_CMIT_MNG
(
    CMIT_MNG_NO          NUMBER(10) NOT NULL ,
    TENURE_NO            NUMBER(10) NOT NULL ,
    RGTR_NO              VARCHAR2(20) NULL ,
    REG_YMD              DATE NULL 
);

ALTER TABLE TB_CMIT_MNG
    ADD CONSTRAINT  PK_TB_CMIT_MNG PRIMARY KEY (CMIT_MNG_NO,TENURE_NO);

CREATE TABLE TB_MFCMM
(
    MFCMM_NO             NUMBER(10) NOT NULL ,
    MFCMM_NM             VARCHAR2(40) NULL ,
    MFCMM_OGDP_NM        VARCHAR2(100) NULL ,
    MFCMM_RLM_CD         VARCHAR2(5) NULL ,
    MFCMM_RSPOFC_NM      VARCHAR2(100) NULL ,
    MFCMM_TELNO          VARCHAR2(11) NULL ,
    MFCMM_BRDT           CHAR(8) NULL ,
    MFCMM_EML_ADDR       VARCHAR2(100) NULL ,
    RGTR_NO              VARCHAR2(20) NULL ,
    REG_YMD              DATE NULL ,
    MDFR_NO              VARCHAR2(20) NULL ,
    MDFCN_YMD            DATE NULL 
);

ALTER TABLE TB_MFCMM
    ADD CONSTRAINT  PK_TB_MFCMM PRIMARY KEY (MFCMM_NO);

CREATE TABLE TB_MFCMM_TENURE
(
    TENURE_NO            NUMBER(10) NOT NULL ,
    MFCMM_NO             NUMBER(10) NULL ,
    CMIT_SE_CD           VARCHAR2(2) NULL ,
    BIZ_AREA_CD          VARCHAR2(10) NULL ,
    TENURE_ODER          VARCHAR2(20) NULL ,
    TENURE_BGNG_YMD      VARCHAR2(8) NULL ,
    TENURE_END_YMD       VARCHAR2(8) NULL ,
    RGTR_NO              VARCHAR2(20) NULL ,
    REG_YMD              DATE NULL ,
    MDFR_NO              VARCHAR2(20) NULL ,
    MDFCN_YMD            DATE NULL 
);

ALTER TABLE TB_MFCMM_TENURE
    ADD CONSTRAINT  PK_TB_MFCMM_TENURE PRIMARY KEY (TENURE_NO);

ALTER TABLE TB_CMIT_MNG
    ADD (CONSTRAINT FK_CMIT_MNG_CMIT_MNG_NO FOREIGN KEY (CMIT_MNG_NO) REFERENCES TB_CMIT_DMGE (CMIT_MNG_NO));

ALTER TABLE TB_CMIT_MNG
    ADD (CONSTRAINT FK_CMIT_MNG_TENURE_NO FOREIGN KEY (TENURE_NO) REFERENCES TB_MFCMM_TENURE (TENURE_NO));

ALTER TABLE TB_MFCMM_TENURE
    ADD (CONSTRAINT FK_MFCMM_TENURE_MFCMM_NO FOREIGN KEY (MFCMM_NO) REFERENCES TB_MFCMM (MFCMM_NO) ON DELETE SET NULL);

COMMENT ON TABLE TB_CMIT_DMGE IS '위원회피해조사';
