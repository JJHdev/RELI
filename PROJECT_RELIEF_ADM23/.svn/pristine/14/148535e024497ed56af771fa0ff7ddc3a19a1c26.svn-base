/* *** 디자인적용 중 정렬문제 수정 *** */
.tableWrap.type1 th {text-align:center;}
/* *** 디자인적용 중 필수표시 추가 *** */
.tableWrap.type1 tr td.must {position:relative;}
.tableWrap.type1 tr td.must span {display:inline-block; position:relative; padding-right:10px;}
.tableWrap.type1 tr td.must span:after {content:'*'; color:#f00; position:absolute; right:0; top:0; line-height:1;}

.formGroup p.must ,.inputGroup p.must  {display:inline-block; padding-right:10px;}
.formGroup p.must:after,.inputGroup p.must:after  {content:'*'; color:#f00; right:0; left:10px; top:0; line-height:1;}

.tableWrap tr th.must {vertical-align:middle; text-align:center; position:relative; display:inline-block; padding-right:10px;}
.tableWrap tr th.must:after {text-align:center; position:relative; content:'*'; color:#f00; right:0; left:10px; top:0; line-height:1;}

.system-form tr td.must {vertical-align:middle; text-align:center; position:relative; display:inline-block; padding-right:10px;}
.system-form tr td.must:after {text-align:center; position:relative; content:'*'; color:#f00; right:0; left:3px; top:0; line-height:1;}


