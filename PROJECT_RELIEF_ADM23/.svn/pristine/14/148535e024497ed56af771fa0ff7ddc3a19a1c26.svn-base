/* 2023.01.17 말풍선 */
.app-balloon-text { position: relative; color: #8EC31F; }
.app-balloon-icon { display: flex; padding: 2px 10px;cursor: pointer;}
.app-balloon-box  { display: none; position: absolute; width: 450px; padding: 8px; left:-250%; -webkit-border-radius: 8px; -moz-border-radius: 8px; border-radius: 8px; background: #333; color: #fff; font-size: 14px; z-index: 100001;}
.app-balloon-box:after { position: absolute; bottom: 100%; left: 45%; width: 0; height: 0; margin-left: -10px; border: solid transparent; border-color: rgba(51, 51, 51, 0); border-bottom-color: #333; border-width: 10px; pointer-events: none; content: ' ';}
.app-balloon-icon:hover + p.app-balloon-box { display: block;}
@media screen and (max-width: 768px) {
	.app-balloon-box { position: relative; width:100%; left: 0;}
	.app-balloon-box:after { left: 10%; }
	.app-balloon-icon:hover + p.app-balloon-box { width:100%; white-space: normal; }
}

