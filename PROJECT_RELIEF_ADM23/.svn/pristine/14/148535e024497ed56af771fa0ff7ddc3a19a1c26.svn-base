/* 2023.01.12 layerpopup.css .layerPop .layerPop-inner  */
.layerPop-wrap { padding:100px 60px 50px; max-width:calc(100% - 40px); position:absolute; left:50%; top:50%; background:#fff;transform:translate(-50%,-50%); border-radius:50px; border:2px solid #e4e4e4; box-shadow:20px 20px 20px rgba(0,0,0,.05);}
.layerPop-wrap .close {position:absolute; right:30px; top:30px; width:40px; height:40px;background:url(../images/common/i-close.svg)center /10px no-repeat;}
.layerPop-inner {max-width:100%;}
.layerPop-inner h3 {font-size:24px; color:#000;font-weight:600; }
.layerPop-inner>* {max-width:100%;}
.layerPop-inner h4 {color:#000; font-weight:600; font-size:19px; padding-bottom:12px;}
.layerPop-inner span.call {padding-left:20px; margin-top:20px; display:inline-block; font-size:12px;color:#958C8A; background:url(../images/apply/i-call.svg)0 50% no-repeat;}
.layerPop-inner span.call b {color:#2e68ff; font-weight:600; }
.layerPop-inner span.mail {padding-left:20px; margin-top:10px; display:inline-block; font-size:12px;color:#958C8A; background:url(../images/apply/i-call.svg)0 50% no-repeat;}
.layerPop-inner span.mail b {font-weight:500; color:#000;}
.layerPop-inner p.err {color:#f00; text-align:center;}
.layerPop-inner p.blue {font-size:16px; color:#2E68FF; text-align:center;}
.layerPop-inner p b {font-size:18px; font-weight:700; color:#000;}
.layerPop-inner p.success {color:#000; font-weight:600; font-size:24px; text-align:center;}
.layerPop-inner .btnWrap.one {position:relative; margin-top:20px;}
.layerPop-inner .btnWrap.one:before {content:''; display:block; width:120%; height:1px; border-top:2px dashed #ddd; position:absolute; left:50%; top:0; transform:translateX(-50%);}
.layerPop-inner .btnWrap.one a {display:block; width:100%; padding:0; margin:0;}
.layerPop-inner .btnWrap.type0 a {height:42px; line-height:42px;}

/* 2023.01.12 layerpopup.css .layerPop.type2, .layerPop.type3, .layerPop.type4,   */
.layerPop-type1 .layerPop-inner .btnWrap a {padding:0 40px; color:#000;height:56px; line-height:56px; border-radius:8px;font-size:16px; background: #F1F3F9; border:0; }
