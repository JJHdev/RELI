.btnWrap.type1 {text-align:center; font-size:0; margin-top:14px;}
.btnWrap.type1 a {display:inline-block; padding:12px 32px; font-size:15px; font-weight:500;line-height:1;border:1px solid; border-radius:10px;background:#fff; color:#012863;}
.btnWrap.type1 a.blue { color:#fff; background:#012863 !important; }

/* 버튼::type2 */
.btnWrap.type2 {text-align:center; font-size:0; margin-top:20px;}
.btnWrap.type2 a {display:inline-block; padding:18px 55px; font-size:16px; font-weight:400; color:#000; background:#F1F3F9; line-height:1;border:1px solid #ddd; border-radius:8px;}
.btnWrap.type2 a:hover {background:#2E68FF; color:#fff;}

.btnWrap.type2 {text-align:right; font-size:0;}
.btnWrap.type2 a {display:inline-block; width:206px; padding:18px 0; border:1px solid #ddd; background:#F1F3F9; color:#000; font-size:17px; border-radius:8px; line-height:1;text-align:center; font-weight:500;}
.btnWrap.type2 a+a {margin-left:10px;}
.btnWrap.type2 a.blue {background:#1E7BA9; color:#fff;}

