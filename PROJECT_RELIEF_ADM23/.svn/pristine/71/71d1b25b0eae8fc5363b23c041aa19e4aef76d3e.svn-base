select {
	padding:0 40px 0 10px;
    -webkit-appearance: none; /* 네이티브 외형 감추기 */
    -moz-appearance: none;
    border:1px solid #D4D4D4;
    appearance: none; cursor:pointer; height:35px; line-height:33px; border-radius:5px;
    background:#FFFFFF url(../../images/common/selectArrow.svg) no-repeat right 20px center; /* 화살표 모양의 이미지 */
}

select.bgWhite {background-color:#fff;}
select::-ms-expand {display: none;} /* IE 10, 11의 네이티브 화살표 숨기기 */
select::focus {outline:none}
