@charset "utf-8";

/* 개발관련 사용자정의 css */
.app-blue-btn {padding: 0 20px;color:#fff !important;background-color:#012863 !important; border-radius: 5px;font-size: 15px;font-weight: 500; margin-left: 10px; margin-right: 10px;}
.app-term-btn+label { width: 60px; padding-left: 15px !important; }
.app-term-date  { width: 210px; }
.app-term-month { width:  87px; margin-left:2px; }
.app-term-year  { width: 121px; }
.app-term-year0 { width: 210px; }
.formLayout .cal .cal-date input { padding-right: 0 !important; }

.app-term-radio { width: 100px !important; }
.app-term-radio label {cursor:pointer;}
.app-term-radio input[type='text'] {-webkit-appearance: none; border:1px solid #e5e5e5;}
.app-term-radio input[type='radio'] {position:absolute; left:-999999px;}
.app-term-radio input[type='radio']+label {display:inline-block;margin-right:15px; color:#000; line-height:35px; padding-left:30px; background:url(../../images/common/agree.svg)0 50% /15px no-repeat; cursor:pointer;}
.app-term-radio input[type='radio']:checked+label {background-image:url(../../images/common/agree-on.svg);}
.app-term-radio input[type='radio']+label:last-child {margin:0;}

.app-search-layout {padding:20px; border-radius:10px; background:#F1F3F9;}
.app-search-label  {width:auto !important; padding-right:10px; }

.app-form-layout {position:relative;}
.app-form-layout label {cursor:pointer;}
.app-form-layout input[type="text"].w100 {width:100%;}
.app-form-layout textarea.w100 {width:100%;}
.app-form-layout button,
.app-form-layout input[type="text"],
.app-form-layout input[type="password"] {padding:0 10px; height:35px; line-height:33px; border-radius:5px; border:1px solid #D4D4D4;background-color:#fff; color:#484848;}
.app-form-layout input[type="checkbox"]+label,
.app-form-layout input[type="radio"]+label {line-height:33px; padding-right:10px;}
.app-form-layout textarea {border-radius:5px; border:1px solid #D4D4D4;background-color:#fff; color:#484848;}
.app-form-layout .formGroup {padding:5px 0;}
.app-form-layout .formGroup.middle>*{vertical-align:middle;}
.app-form-layout .formGroup.top>*{vertical-align:top;}
.app-form-layout .formGroup p {display:inline-block; padding-left:10px; width:150px; color:#171717; font-size:15px; font-weight:500;}
.app-form-layout .formGroup span.label {display:inline-block; padding-left:10px; color:#171717; font-size:15px; font-weight:500; line-height:1.2;}
.app-form-layout .formGroup .inputWrap {display:inline-block;}
.app-form-layout .formGroup .inputWrap span {display:inline-block;padding:0 20px;color:#171717; font-size:15px; font-weight:500;}
.app-form-layout .formGroup .inputWrap span.bar {padding:0; width:20px; color:#ABB8BC; font-size:15px; font-weight:500; text-align:center;}
.app-form-layout .formGroup .inputWrap+.inputWrap {margin-left:30px ;}
.app-form-layout .formGroup .inputWrap .btn {padding:0 20px; color:#fff; background:#96D019; border:0;}
.app-form-layout .formGroup .inputWrap .btn.blue {background:#2E68FF;}
.app-form-layout .formGroup .inputWrap .btn.black {background:#000;}
.app-form-layout .cal>div {display:inline-block;}
.app-form-layout .cal .cal-date {position:relative;}
.app-form-layout .cal .cal-date input {padding-right:40px;}
.app-form-layout .cal .cal-date input::disabled {background:inherit;}
.app-form-layout .cal .cal-date a {position:absolute; right:0; top:0;width:35px; height:100%; background:url(../../images/common/i-cal.svg)center no-repeat;}
.app-form-layout .btnCheck input[type="radio"],
.app-form-layout .btnCheck input[type="checkbox"] {position:absolute; left:-999999999px;}
.app-form-layout .btnCheck input[type="radio"]+label,
.app-form-layout .btnCheck input[type="checkbox"]+label {padding:10px 15px; font-size:13px; font-weight:500; color:#BCC7CA; border:1px solid; border-radius:5px; line-height:1; cursor:pointer; display:inline-block;}
.app-form-layout .btnCheck input[type="radio"]:checked+label,
.app-form-layout .btnCheck input[type="checkbox"]:checked+label {color:#2E68FF;}

