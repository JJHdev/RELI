/* viewWrap 응용 */
.app-view         {background-color:#fff; border:1px solid #e5e5e5;}
.app-view>div     {position:relative; display:table; width:100%; }
.app-view>div>*   {display:table-cell; vertical-align:middle; padding:10px 20px;}
.app-view>div+div {border-top:1px solid #e5e5e5;}
.app-view>div b   {width:150px;font-weight:500; color:#444;text-align:center;background:#f9f9f9;}


.app-small-grid .datagrid-toolbar span {padding:0 !important; }
.app-small-grid .panel-body {font-size: 13px;}
.app-small-grid .panel-title {font-size: 13px; height: 30px;line-height: 30px;}
.app-small-grid .datagrid-mask-msg {height: 16px;}
.app-small-grid .datagrid-empty {height: 30px; line-height: 30px;}
.app-small-grid .datagrid-btn-separator {height: 30px;}
.app-small-grid .datagrid-header-row,
.app-small-grid .datagrid-row { height: 34px;}
.app-small-grid .datagrid-cell,
.app-small-grid .datagrid-cell-group,
.app-small-grid .datagrid-header-rownumber,
.app-small-grid .datagrid-cell-rownumber { height: 30px; line-height: 30px;  font-size: 13px;}
.app-small-grid .datagrid-view .datagrid-editable-input { font-size: 13px;}
.app-small-grid .pagination-btn-separator {height: 30px;}
.app-small-grid .l-btn-text { line-height: 30px;}

.app-small-grid .l-btn span span .l-btn-empty { height: 30px;}
.app-small-grid input[type="text"] { height: 30px; line-height:30px; }

.app-edit-grid .datagrid-row-editing .textbox { color: black !important; }

