.app-balloon-icon:hover + p.app-balloon-box { display: block;}
@media screen and (max-width: 768px) {
	.app-balloon-box { position: relative; width:100%; left: 0;}
	.app-balloon-box:after { left: 10%; }
	.app-balloon-icon:hover + p.app-balloon-box { width:100%; white-space: normal; }
}
