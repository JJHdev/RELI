@charset "utf-8";

/* 메인페이지 스타일만 정의 */



/* ************************ PC (992~) ************************ */
@media screen and (min-width: 992px) {
	
	.main {background:#f2f5fa;}
	/* .main .visual {height:750px; background:url(../../images/main/main_bg.jpg)center /cover no-repeat;} */
	/* .main .visual {height:800px; background:url(../../images/main/main_bg-01.jpg)center /cover no-repeat;} */
	.main .visual {height:700px; background:url(../../images/main/main_bg-01.jpg)center /cover no-repeat;}
	.main .visual-wrap {max-width:1200px;margin:0 auto;}
	.main .visual-wrap>h2 {padding:60px 0 0; font-size:35px; font-weight:800; text-align:center; color:#014B7C; letter-spacing:-1px;}
	.main .visual-wrap>p {padding:15px 0 0;font-size:18px; font-weight:600; text-align:center; color:#202020; line-height:1.3; letter-spacing:-1px; }
	.main .visual .swiper {padding:60px 0 50px; overflow:hidden; }
	.main .visual .swiper-wrapper {}
	.main .visual .swiper-slide {padding:50px; display:block; background:#edf0f5; color:#5e676e; border-radius:10px; transition:all .2s; position:relative;}
	.main .visual .swiper-slide.on,
	.main .visual .swiper-slide:hover {padding:50px; background:#fff; color:#000; transform:translateY(-20px);box-shadow: 0 30px 30px rgba(0, 0, 0, .2);}
	.main .visual .swiper-slide.on i,
	.main .visual .swiper-slide:hover i {bottom:60px;}
	.main .visual .swiper-slide dl {padding:0 0 60px;}
	.main .visual .swiper-slide dt {padding:0 0 18px; font-size:22px; font-weight:800;}
	.main .visual .swiper-slide dd {}
	.main .visual .swiper-slide a {padding:16px 30px; display:inline-block; line-height:1; color:#fff; border-radius:5px; font-size:18px;}
	.main .visual .swiper-slide form {}
	.main .visual .swiper-slide form input {padding:0 20px; width:100%; height:46px; line-height:44px; border:1px solid #000;}
	.main .visual .swiper-slide form button {padding:16px 30px; display:inline-block; line-height:1; color:#fff; border-radius:5px;font-size:18px;}
	.main .visual .swiper-slide .bgc1 {background-color:#7db0b1;}
	.main .visual .swiper-slide .bgc2 {background-color:#96d019;}
	.main .visual .swiper-slide .bgc3 {background-color:#b18c7c;}
	.main .visual .swiper-slide i {position:absolute; right:50px; bottom:50px; transition:all .2s; text-align:right;}
	.main .visual .swiper-slide i img {width:75%;}
	.main .swiper-button-next,
	.main .swiper-button-prev {width:64px; height:64px; background-color:#fff; background-size:15px; border-radius:50%; top:53%;}
	.main .swiper-button-prev {left:14%; background-image:url(../../images/main/swiper_arrow_left.jpg);}
	.main .swiper-button-next {right:14%; background-image:url(../../images/main/swiper_arrow_right.jpg);}
	.main .visual .swiper-pagination {display:none;}
	
	/* .main .info {background:#f2f5fa; padding:30px 0 80px;} */
	.main .info { padding:40px 0 80px; margin-top:-160px;}
	.main .info-wrap {width:1200px; max-width:100%; margin:0 auto; overflow:hidden;}
	.main .info-wrap>* {float:left;}
	.main .info-left {width:66.6666%;}
	.main .info-left-list {overflow:hidden;}
	.main .info-left-list-item {float:left; width:calc((100% - 25px)/2); padding:50px 40px; background:#fff;border-style:solid; border-width:4px 0 0 0; border-color:#2F4D9A; position:relative;}
	.main .info-left-list-item+.info-left-list-item {margin-left:25px;}
	/* .main .info-top-list:hover {background:#f9f9f9;} */
	.main .info-left-list-item i {position:absolute; right:0; bottom:0;}
	.main .info-left-list-item p {color:#325780; font-size:18px; font-weight:800; padding:0 0 25px;}
	.main .info-left-list-item .btn {
		display:inline-block; color:#fff; background:#2F4D9A; border-radius:10px; font-size:16px; font-weight:700; line-height:1;
		padding:20px; box-shadow:0 2px 5px rgba(0,0,0,.2); transition:all .2s;
	}
	
	.main .info-right {width:33.3333%; padding-left:25px;}
	
	
	.main .info-link {}
	.main .info-link .link {padding-top:25px;}
	.main .info-link a {padding:25px; background:#fff url(../../images/main/portal_arrow.png)right 13px center no-repeat; display:block; border:1px solid #008b8a; font-size:18px; font-weight:500; color:#666;}

	.main .videoSwiper {margin-top:25px; height:445px; overflow:hidden; position:relative; cursor:pointer;}
	.main .videoSwiper .swiper-slide {background-size:cover; background-position:center;}
	.main .videoSwiper .swiper-pagination {bottom:auto; top:30px; left:30px; text-align:left;}
	.main .videoSwiper .swiper-pagination-bullet {
		width: 30px;height: 30px;text-align: center;line-height: 30px;font-size: 15,px;
		color: #fff;opacity: 1;background: rgba(0, 0, 0, 0.1);
    }
    .main .videoSwiper .swiper-pagination-bullet-active {color: #000;background: #fff;}
	.main .videoSwiper .cover {position:absolute; left:0; top:0; right:0; bottom:0; background:rgba(0,0,0,0.5);}
	.main .videoSwiper:hover .cover {background:rgba(0,0,0,0.2);}
	.main .videoSwiper p {color:#fff;position:absolute; top:30px; left:157px; z-index:9; font-size:21px; font-weight:200;}
	.main .videoSwiper p b {font-weight:500;}
	.main .videoSwiper i {position:absolute; left:50%; top:50%; transform:translate(-50%,-50%); z-index:9;}
	.main .videoSwiper i img {width:50px;}
	.main .info-call {padding:50px 40px; height:216px; border-top:4px solid #14303E; background:#fff url(../../images/main/call_bg.png)center bottom no-repeat; position:relative;}
	.main .info-call a {display:block; font-weight:700; font-size:24px; color:#484848;}
	.main .info-call a span {color:#014B7C;}
	.main .info-call p {padding-top:20px;}
	.main .info-call p span {color:#26779B;}
	.main .info-call img {position:absolute; right:0; bottom:0; opacity: 0.4;}
	
	.mainLayer {z-index:999999999; position:fixed; left:0; top:0; width:100%; height:100vh; display:none;}
	.mainLayer.on {display:block;}
	.mainLayer .cover {position:absolute; left:0; top:0; width:100%; height:100%; background:rgba(0,0,0,.5);}
	.mainLayer-wrap {width:700px; position:absolute; left:50%; top:50%; background:#fff;transform:translate(-50%,-50%); border-radius:10px;}
	.mainLayer-tit {position:relative; padding:12px 90px 12px 20px; border-bottom:1px solid rgba(0,0,0,.2);}
	.mainLayer-tit p {font-size:24px; color:#000;}
	.mainLayer-tit button.close {position:absolute; right:0; top:0; width:60px; height:100%; border-left:1px solid rgba(0,0,0,.2); background:url(../../images/common/i-close.svg)center /10px no-repeat;}
	.mainLayer-inner {padding:50px;}
	.mainLayer-inner video {width:100%;}
	
    /* ntarget 2022-10-14 배너 추가 */
    .main .banner {padding-top:25px;}
    .main .banner .link {padding-top:25px;}
    .main .banner a {padding:25px; background:#fff url(../../images/main/banner01.jpg) center no-repeat; background-size:cover; height:175px; display:block; border:1px solid #008b8a; font-size:18px; font-weight:500; color:#666;}
        	
}



