/* ************************ MOBILE (~768) ************************ */
@media screen and (max-width: 768px) {
	
	.main .visual {padding:0 0 50px;}
	.main .visual-wrap {padding:0 20px;}
	.main .visual-wrap>h2 {padding:50px 0 0; font-size:27px;}
	.main .visual-wrap>p {padding:15px 0 0; font-size:17px; font-weight:500;}
	
	.main .visual .swiper {padding:30px 0 0;}
	.main .visual .swiper-slide {position:relative; padding:30px;}
	.main .visual .swiper-slide a {padding:30px;}
	.main .visual .swiper-slide a {padding:10px 20px; position:absolute; left:30px; bottom:30px;}
	.main .visual .swiper-slide form button {padding:10px 20px;}
	.main .visual .swiper-slide dl {padding:0 0 70px;}
	.main .visual .swiper-slide dt {font-size:19px; padding:0 0 15px;}
	.main .visual .swiper-slide i {right:30px; bottom:30px; text-align:right;}
	.main .visual .swiper-slide i img {width:70%;}
	.main .swiper-button-next,
	.main .swiper-button-prev {display:none;}
	.main .visual .swiper-pagination {display:block;}
	.main .visual .swiper-pagination {bottom:20px;}
	.main .visual .swiper-pagination-bullet {width:10px; height:10px; }
	.main .visual .swiper-pagination-bullet-active {background:#fff;}
	
	.main .info {padding:30px 20px 50px;}
	.main .info-left-list-item {width:100%; padding:30px 20px;}
	.main .info-left-list-item+.info-left-list-item {margin:20px 0 0;}
	.main .info-left-list-item p {font-size:17px;}
	.main .info-left-list-item .btn {font-size:15px; padding:15px 20px;}
	.main .info-left-list-item i {text-align:right;}
	.main .info-left-list-item i img {width:70%;}
	
	.main .videoSwiper p {left:20px; top:55px; font-size:16px;}
	.main .videoSwiper .swiper-pagination {top:20px; left:20px;}
	.main .videoSwiper .swiper-pagination-bullet {width:24px; height:24px; line-height:24px; font-size:14px;}
	.main .videoSwiper i img {width:30px;}
	
	
	.main .info-right>div {width:100%;}
	.main .info-call {padding:30px 20px; height:auto; background-position:center calc(100% + 100px);}
	.main .info-call a {font-size:21px;}
	.main .info-call p {padding-top:12px;}
	.main .info-call img {width:30%;}
	.main .info-link {padding-left:0; margin-top:0;}
	.main .info-link .link {}
	.main .info-link a {padding:20px; font-size:16px;}
	
	/* ntarget 2022-10-14 배너 추가 */
    .main .banner {padding-top:25px;}
    .main .banner .link {padding-top:25px;}
    .main .banner a {padding:25px; background:#fff url(../../images/main/banner01.jpg) center no-repeat; background-size:cover; height:175px; display:block; border:1px solid #008b8a; font-size:18px; font-weight:500; color:#666;}
       
	
	
	
	
	.mainLayer-tit p {font-size:19px;}
	.mainLayer-inner {padding:30px;}
	
	
}

/* ************************ 작은 MOBILE (~450) ************************ */
@media screen and (max-width: 450px) {
	
	.main .visual .swiper-slide dl {padding:0 0 100px;}
	
	
		
}


















