$(function() {
    //========================================================//
    // 화면 스크립트 내에 사용할 객체,상수 정의
    //--------------------------------------------------------//

    //========================================================//
    // FUNCTION 정의
    //--------------------------------------------------------//

    // 상세화면 이동
    //--------------------------------------------------------//
    function doViewLwstAply(aplyNoVal){
        $.formUtil.submitForm(
            getUrl('/usr/mypage/viewLwst.do'), {
                params: { aplyNo: aplyNoVal }
            }
        );
    }

    //========================================================//
    // FORM ELEMENT EVENT 정의
    //--------------------------------------------------------//

    $("tr.btnView").on("click",function(){
        var aplyNoVal = $(this).data("aplyno");
        doViewLwstAply(aplyNoVal);
    });
});
