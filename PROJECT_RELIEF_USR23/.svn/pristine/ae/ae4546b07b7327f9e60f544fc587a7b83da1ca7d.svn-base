/* ***** tabWrap ***** */
.tabWrap {}
/* ***** tabWrap::type1 ***** */
.tabWrap.type1 {}
.tabWrap.type1 ul {font-size:0; text-align:center;}
.tabWrap.type1 ul li {display:inline-block; width:270px; margin:0 20px; font-size:18px;}
.tabWrap.type1 ul li a {display:block; padding:18px 0; background:#F1F3F9; border-radius:50px; font-weight:400;}
.tabWrap.type1 ul li.on a {color:#fff; font-weight:600;background:#2E68FF; }
/* ***** tabWrap::type2 ***** */
.tabWrap.type2 {padding-bottom:50px;}
.tabWrap.type2 ul {text-align:center; border-width:1px 0 0 1px; border-style:solid; border-color:#e5e5e5;}
.tabWrap.type2 ul li {float:left; border-width:0 1px 1px 0; border-style:solid; border-color:#e5e5e5;}
.tabWrap.type2 ul li button {width:100%; line-height:54px; transition:all .2s;}
.tabWrap.type2 ul li button:hover {background:#f5f5f5;}
.tabWrap.type2 ul li.on button {color:#fff; background:#00b38d;}
.tabWrap.type2.li-2 ul li {width:calc(100%/2)}
.tabWrap.type2.li-3 ul li {width:calc(100%/3)}
.tabWrap.type2.li-4 ul li {width:calc(100%/4)}
.tabWrap.type2.li-5 ul li {width:calc(100%/5)}
.tabWrap.type2.li-6 ul li {width:calc(100%/6)}
/* ***** tabWrap::type3 ***** */
.tabWrap.type3 {}
/* .tabWrap.type3 ul {font-size:0; text-align:center;} */
.tabWrap.type3 ul li {float:left; font-size:18px;}
.tabWrap.type3 ul li+li {margin-left:30px;}
.tabWrap.type3 ul li a {display:block; padding:16px 0; background:#F1F3F9; border-radius:50px; font-weight:400; font-size:18px; text-align:center;}
.tabWrap.type3 ul li.on a {color:#fff; font-weight:600;background:#2E68FF;}
.tabWrap.type3 ul.li-2 li {width:calc((100% - 30px)/2)}
.tabWrap.type3 ul.li-3 li {width:calc((100% - 60px)/3)}
.tabWrap.type3 ul.li-4 li {width:calc((100% - 90px)/4)}
.tabWrap.type3 ul.li-5 li {width:calc((100% - 120px)/5)}
.tabWrap.type3 ul.li-6 li {width:calc((100% - 150px)/6)}

/* ***** tabWrap::type4 ***** */
.tabWrap.type4 {width:1200px; max-width:100%; margin:0 auto 80px;}
/* .tabWrap.type3 ul {font-size:0; text-align:center;} */
